<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:sensetime="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/csv_camera_layout"
        android:layout_width="match_parent"
        android:layout_height="580px"
        android:layout_marginTop="10dp"
        android:padding="0dp">

        <com.yilong.mvp.ZKCameraTextureView
            android:id="@+id/mCameraView"
            android:layout_width="match_parent"
            android:layout_height="580px"
            android:layout_centerHorizontal="true"
            sensetime:cameraFacing="back"
            sensetime:resolution="480"
            sensetime:scale="scale_16_9" />

        <com.yilong.mvp.view.FaceSurfaceView
            android:id="@+id/mOverlapView"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </RelativeLayout>


</LinearLayout>
